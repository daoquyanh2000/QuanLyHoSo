@model IEnumerable<QuanLyHoSo.Models.PDFThanhPhanHoSo>
<table class="table" id="PDFTable">
    <thead>
        <tr>
            <th>Tên File</th>
            <th>Chức năng </th>
        </tr>

    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr data-id="@item.ID">
                <td>
                    @item.TenPDF
                </td>
                <td>
                    <div class="btn-group btn-group-sm" role="group" aria-label="Basic mixed styles example">
                        <button type="button" class="btn btn-outline-secondary ViewPDF" data-id="@item.ID">
                            <span>Xem</span>
                        </button>
                        <button type="button" class="btn btn-outline-secondary DeletePDF" data-id="@item.ID" >
                            <span>Xóa </span>
                        </button>
                    </div>
                </td>
            </tr>
        }


    </tbody>
</table>
